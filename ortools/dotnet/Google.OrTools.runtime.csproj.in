      $<@need_cbc_lib@:;$<TARGET_SONAME_FILE:Coin::Cbc>;$<TARGET_SONAME_FILE:Coin::OsiCbc>;$<TARGET_SONAME_FILE:Coin::CbcSolver>>

      $<@need_unix_highs_lib@:;$<TARGET_SONAME_FILE:highs>>
